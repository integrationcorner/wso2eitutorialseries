<?xml version="1.0" encoding="UTF-8"?>
<api context="/messagestoreapi" name="MessageStoreAPI" xmlns="http://ws.apache.org/ns/synapse">
    <resource methods="POST">
        <inSequence>
            <log level="custom">
                <property name="LogPoint" value="Message Received"/>
            </log>
            <store messageStore="SampleMS"/>
            <log level="custom">
                <property name="LogPoint" value="Message Posted to Message Store"/>
            </log>
            <respond/>
        </inSequence>
        <outSequence/>
        <faultSequence/>
    </resource>
</api>
